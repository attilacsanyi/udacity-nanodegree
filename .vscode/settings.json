{
  // Place your settings in this file to overwrite default and user settings.
  // Files
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true
  },
  "files.autoSave": "onFocusChange",
  // Search
  "search.exclude": {
    ".vscode": true,
    "**/node_modules": true,
    "**/*.d.ts": true,
    "**/*.js": true
  },
  // Editor
  "window.title": "${rootName}${separator}${activeEditorLong} ${dirty}",
  "editor.formatOnSave": true
}
